page{
    font-size: 14px;
}
.bt{
    display: flex;
    justify-content: center;
    font-size: 15px;
}
.su-table-box {
    height: 100%;
    margin: 0rpx 30rpx;
    overflow-x: auto;
    padding-bottom: 40rpx;
}
.su-table {
    /* margin-top: 50rpx; */
    display: table;
    border-collapse: collapse; 
    overflow-x: auto;
    word-break: keep-all;
    white-space: nowrap;
    background-color: #FFFFFF;
}
.su-tr {
    display: table-row;
}
.su-th {
    display: table-cell;
    height: 60rpx;
    line-height: 60rpx;
    text-align: center;
    border: 1rpx solid #DDDDDD;
    border-right: none;
    border-bottom: none;
    background-color: #87CEFA;
    color: #FFFFFF;
}
.su-td {
    padding: 0rpx 30rpx;
    display: table-cell; 
    height: 60rpx;
    line-height: 60rpx;
    text-align: center;
    border: 1rpx solid #DDDDDD;
    /* border-right: none; */
}
.su-td-last {
    padding: 0rpx 30rpx;
    display: table-cell; 
    height: 60rpx;
    line-height: 60rpx;
    text-align: center;
    border: 1rpx solid #DDDDDD;
}

